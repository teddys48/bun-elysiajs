{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/bun",
        "config": { "includeFiles": ["dist/**"] }
      },
      {
        "src": "/tmp/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts"
      },
      {
        "src": "/(.+)",
        "dest": "/tmp/$1"
      }
    ]
  }
  